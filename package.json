{
  "name": "storybook-addons-abstract",
  "version": "5.0.7",
  "description": "Abstract previews for storybook",
  "keywords": [
    "storybook",
    "design",
    "abstract",
    "sketch",
    "xd"
  ],
  "homepage": "https://github.com/amccloud/storybook-addons-abstract#readme",
  "bugs": {
    "url": "https://github.com/amccloud/storybook-addons-abstract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amccloud/storybook-addons-abstract.git"
  },
  "license": "MIT",
  "files": [
    "dist/**/*",
    "docs/**/*",
    "README.md",
    "register.js"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "lint": "eslint .",
    "check": "tsc --noEmit",
    "test": "yarn check && yarn lint",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "now-build": "yarn build && build-storybook --output-dir ./public",
    "now-dev": "start-storybook -p $PORT",
    "release": "standard-version"
  },
  "dependencies": {
    "@storybook/addons": "^5.2.8",
    "@storybook/api": "5.3.0-alpha.11",
    "@storybook/client-logger": "5.3.0-alpha.11",
    "@storybook/components": "5.3.0-alpha.11",
    "@storybook/core-events": "5.3.0-alpha.11",
    "@storybook/theming": "5.3.0-alpha.11",
    "abstract-sdk": "^8.0.0-beta.32",
    "core-js": "^3.0.1",
    "global": "^4.3.2",
    "prettier": "^1.18.2",
    "query-string": "^6.13.1",
    "react": "^16.8.3",
    "ts-dedent": "^1.1.0",
    "use-image": "^1.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "standard-version": "^8.0.2",
    "storybook-abstract-addon": "link:./",
    "typescript": "^3.6.3"
  }
}
